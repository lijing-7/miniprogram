/* 轮播图结束 */
/*公用title begin*/
.floor-title {
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  color: #222;
  font-size: 34rpx;
  font-weight: bold;
  position: relative;
  background-color: white;
}
.floor-title text {
  color: #006699;
  font-size: 30rpx;
  padding-right: 14rpx;
}
.floor-title icon{
position: absolute;
  top: 40rpx;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  border-top: 10rpx solid transparent;
  border-bottom: 10rpx solid transparent;
  border-left: 10rpx solid #222222;
}
.todayclass-title icon{
  top: 40rpx;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  border-top: 10rpx solid transparent;
  border-bottom: 10rpx solid transparent;
  border-left: 10rpx solid #222222;
}
/*公用title end*/
/*拼团begin*/
.fine-puzzle{background-color: white;}
.puzzle-list{
    padding-bottom: 20rpx;
    display: flex;
    white-space: nowrap;
}
.puzzle-list .puzzle-li{
  width: 83.2%;
  margin-left: 4%;
  border: 1px solid #f6f6f6;
  position: relative;
  display: inline-block;
}
.puzzle-list .puzzle-li:last-child{margin-right: 4%;}
.puzzle-img,.puzzle-img image{
  width: 100%;
  height: 333rpx;
}
.plr20{
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.puzzle-name{
    font-size: 30rpx;
    line-height: 40rpx;
    font-weight: bold;
    color: #222222;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.puzzle-ft{
  height: 70rpx;
  line-height: 70rpx;
  margin-bottom: 1rpx;
}
.puzzleft-l{width: 55%;}
.puzzleft-l text{
  float: left;
  color: #555555;
  font-size: 22rpx;
}
.puzzleft-l text:first-child{color: #0a0a0a;}
.puzzleft-l text.puzzle-ll-text3{margin-right: 18rpx;}
.puzzleft-r{width: 100%;}
.puzzleft-r text{
  float: right;
  display: block;
  text-align: center;
}
.puzzleft-r text:first-child{
  width: 30%;
  height: 64rpx;
  line-height: 64rpx;
  /* background-color: rgb(216, 201, 201); */
  font-size: 22rpx; 
  color: #0a0a0a;
  /* border: 1px dashed #faf6f4; */
}
/*拼团end*/
.todayclass{
  width: 100%;
  height:auto;
  background-color: white;
}
.todayclass-title{
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  color: #222;
  font-size: 34rpx;
  font-weight: bold;
  position: relative;
  background-color: white;
  /* width: 100%; */
  margin-top: 30rpx;
}
.todayclass-title text{
  color: #006699;
  font-size: 30rpx;
  padding-right: 14rpx;
  padding-left: 14rpx;
}
.todayclass-list{
  width: 100%;
  /* height: 390rpx; */
  background-color: white;
}
/* 时间背景 */
.todayclasslist-set{
  width: 100%;
  height: 120rpx;
  margin-bottom:10rpx;
  background-color:white;
  color: #0a0a0a;
}
.todayclasslist-time{
  width: 14%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 22rpx;
  background-color: rgb(172, 223, 247);
  float: left;
  border-bottom-left-radius: 1em;
  border-top-left-radius: 1em;
  margin: 15rpx auto;
  margin-left: 10rpx;
} 
.todayclasslist-cname{
  width: 64%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  background:  rgb(172, 223, 247);
  float: left;
  color: #0a0a0a;
  /* padding-top: 15rpx; */
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  display: block; 
  border-bottom-right-radius: 1em;
  border-top-right-radius: 1em;
  margin: 15rpx auto;
}
.todayclasslist-entry{
  width: 17%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 26rpx;
  background-color: rgb(243, 196, 181);
  float: right;
  border-radius: 2em ;
  margin: 25rpx 8rpx;
}
/* 今日课程end */
/* 作业start */
